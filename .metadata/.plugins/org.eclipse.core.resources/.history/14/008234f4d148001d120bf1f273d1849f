import java.util.Scanner;

public class EstruturaDeRepetiçãoExercicio1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int x;
		int y;
		
		System.out.println("Vamos classisifcar os numeros por sua ordem numerica");
		
		System.out.println("Escolha o primeiro numero: ");
		x = sc.nextInt();
		
		System.out.println("Escolha o segundo numero: ");
		y = sc.nextInt();
		
		while (x != y) {
			if (x < y)
			System.out.println("Ordem crescente!");
			else
			System.out.println("Ordem descrescente");	
			
		}
		
		sc.close();
	}

}
