import java.util.Scanner;

public class ExercicioUri {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner (System.in);
		
		int A, B, X;
		
		
		A = sc.nextInt();
		B = sc.nextInt();
		
		
		X = A + B;
		
		System.out.println(" X = "+X);		
		
		sc.close();

	}

}
