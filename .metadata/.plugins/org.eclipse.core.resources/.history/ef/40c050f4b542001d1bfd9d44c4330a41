		Scanner sc = new Scanner (System.in);
		
		/*
		String x;
		x = sc.next();
		System.out.println("Voce digitou: "+x);
		*/
		
		/*
		int x;
		
		x = sc.nextInt();
		System.out.println("Voce digitou: "+x);
		*/
		
		double x;
